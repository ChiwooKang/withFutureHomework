package homework;

import java.math.BigDecimal;

class Solution {
    public String solution(String s) {
        String[] arr = s.split(" ");
        BigDecimal min = new BigDecimal(Float.MAX_VALUE);
        BigDecimal max = new BigDecimal(Float.MIN_VALUE);
        for (String num : arr) {
        BigDecimal n = new BigDecimal(num);
            if (n.compareTo(min) < 0) {
            min = n;
        }   
            if (n.compareTo(max) > 0) {
            max = n;
        }
    }
    return min + " " + max;
    }
    
	public static void main(String[] args) {
		
		Solution solution = new Solution();
		solution.solution("111111111111111111111111111111111111111111111111111111 111111111111111111111111111111111111111111111111111111");
		System.out.println(solution.solution("111111111111111111111111111111111111111111111111111111 111111111111111111111111111111111111111111111111111111"));
	}

}
